{
  "name": "react-redux-spotify-browser-example",
  "version": "1.0.0",
  "private": true,
  "description": "Example app that showcases the usage of React (0.14), React-Router (1.0) and Redux (1.0)",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test:build": "node node_modules/webpack/bin/webpack.js -p --config webpack.test.config.js",
    "test:run": "mocha --compilers js:babel/register .test/test.bundle.js --require ./test/init.js",
    "test": "npm run test:build && npm run test:run",
    "test-bamboo": "npm run test:build && npm run test:run -- -R mocha-bamboo-reporter",
    "test-debug": "npm run test:build && npm run test:run -- --debug-brk",
    "build": "NODE_ENV=production node node_modules/webpack/bin/webpack.js -p --config webpack.production.config.js"
  },
  "author": "Joonas Lehtonen",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.2",
    "chai": "3.0.0",
    "chai-as-promised": "^5.1.0",
    "css-loader": "^0.15.6",
    "eslint": "^1.1.0",
    "eslint-plugin-react": "^3.2.1",
    "expose-loader": "^0.7.0",
    "file-loader": "^0.8.4",
    "glob": "^5.0.15",
    "html-webpack-plugin": "^1.6.1",
    "imports-loader": "^0.6.4",
    "jsdom": "^6.5.1",
    "mocha-bamboo-reporter": "^1.1.0",
    "node-sass": "^3.2.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^0.14.0",
    "react-hot-loader": "^1.2.8",
    "redux-devtools": "^1.0.2",
    "sass-loader": "^2.0.0",
    "sinon": "^1.17.1",
    "sinon-as-promised": "^4.0.0",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  },
  "dependencies": {
    "classnames": "^2.1.3",
    "font-awesome": "^4.4.0",
    "font-awesome-webpack": "0.0.3",
    "lazysizes": "^1.1.5",
    "minigrid": "^1.6.1",
    "mocha": "^2.3.3",
    "mocha-loader": "^0.7.1",
    "node-bourbon": "^4.2.3",
    "node-neat": "^1.7.2",
    "normalize.css": "^3.0.3",
    "object-fit": "^0.4.2",
    "react": "^0.14.0-beta2",
    "react-dom": "^0.14.0-beta2",
    "react-redux": "0.9.0",
    "react-router": "1.0.0-beta3",
    "redux": "1.0.1",
    "redux-react-router": "^0.2.1",
    "redux-thunk": "^0.1.0",
    "whatwg-fetch": "^0.9.0"
  }
}
